    <add name="EAMIData" connectionString="server=DHITSAWSSQLID11; Trusted_Connection=yes; database=EAMI-MC; connection timeout=30" providerName="System.Data.SqlClient"/>
    <!--<add name="EAMIUIAuthDatabase" connectionString="server=localhost; Trusted_Connection=yes; database=EAMI; connection timeout=30" providerName="System.Data.SqlClient" />-->
  </connectionStrings>
  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true"/>
    <add key="DomainName" value="intra.dhs.ca.gov"/>
    <add key="MailServer" value="smtpoutbound.dhs.ca.gov"/>
    <add key="MailServerPort" value="25"/>
    <add key="MailServerDefaultEmailID" value="NoReply@dhcs.ca.gov"/>
    <add key="IsProductionEnvironment" value="false"/>
    <add key="WebUISvcAuthorizationEnabled" value="false"/>
    <add key="MemoryCacheSlidingExpirationInMinutes" value="30"/>
  </appSettings>
