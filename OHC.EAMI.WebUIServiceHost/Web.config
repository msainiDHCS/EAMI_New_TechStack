    </httpProtocol>
    <validation validateIntegratedModeConfiguration="false"/>
    <security>
      <!--allow only specific AD groups-->
      <authorization>
        <!--<remove users="*" roles="" verbs="" />
        <remove users="?" roles="" verbs="" />
        <add accessType="Deny" users="?" />
        <add accessType="Allow" roles="DHSINTRA\EAMI_USERS" />-->
